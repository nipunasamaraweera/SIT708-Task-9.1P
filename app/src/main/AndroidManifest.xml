            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDb7TLex6gjHVmsOMZUYwzqQcUDMsgMY0Y" />

        <activity
            android:name=".Lost_Found"
            android:exported="false" />
        <activity
            android:name=".ItemData"
            android:exported="false" />
